<h1>Your Bookings</h1>
<ul>
  <% @bookings.each do | booking | %>
    <% if booking.user_id == current_user.id %>
      <li class="card">
        <h4> <%= Listing.find_by_id(booking.listing_id).title %> </h4>
        <p> Starts on: <%= booking.start_at %> </p>
        <p> Ends on: <%= booking.end_at %> </p>
      </li>
    <% end %>
  <% end %>
</ul>
