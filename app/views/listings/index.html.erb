<h1> Welcome to the BabySitter's Club </h1>
<h4> The few, the proud, the babysitters </h4>

<%# <%= link_to "Log Out", destroy_user_session_path, data: { turbo_method: :delete } %>
<%# <%= link_to "Sign_up", new_user_registration_path %>
<ul>
  <% @listings.each do |listing| %>
    <li class="card">
      <h5> <%= listing.title %> <h5>
      <p> <%= listing.description %> </p>
      <p> <%= listing.fee %> </p>
      <p> <%= User.find_by_id(listing.user_id).first_name %> <%= User.find_by_id(listing.user_id).last_name %> </p>
      <p> <%= link_to "Check the details", listing_path(listing) %> </p>
      <% listing.photos.each do |photo| %>
        <%= image_tag photo %>
      <% end %>
    </li>
  <% end %>
</ul>
